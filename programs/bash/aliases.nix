{
  SS = "unset __ETC_BASHRC_SOURCED && . /etc/bashrc";
  du1 = "du -h -d 1";
  fgj = "fg '%-'";
  finame = "find . -name";
  gpc = "git log origin/master.. -- | wc -l";
  gpp = "git log origin/master.. --reverse -p";
  gpr = "git log origin/master.. --reverse";
  grep = "grep --color=auto";
  gst = "git status";
  l = "ls -Alh";
  la = "ls -A";
  ll = "ls -l";
  ls = "ls --color=tty";
  t = "tmux";
  tls = "tmux ls";
}
